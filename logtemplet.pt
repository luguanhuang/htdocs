		<thead>
			<tr>
				<td class="datetime">登录时间</td> 	 	 	
				<td class="ip">登录IP</td>
				<td>事件描述</td>
			</tr>
	  	</thead>
		<tbody>
		<!--start:log_body-->
		<!--start:log_row-->
			<tr class="row%u">
				<td>%s</td>
				<td>%s</td>
				<td class="longtext">%s</td>
			</tr>
		<!--end:log_row-->
		<!--end:log_body-->
		</tbody>
	</table>
	<!--end:log-login-->
	<!--start:log-online-->
