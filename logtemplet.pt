				<td class="port">服务端口</td>
				<td>操作信息</td>
			</tr>
	  	</thead>
		<tbody>
		<!--start:log_body-->
		<!--start:log_row-->
			<tr class="row%u">
				<td>%s</td>
				<td>%s</td>
				<td>%s</td>
				<td>%s</td>
				<td>%s</td>
				<td>%s</td>
				<td class="longtext">%s</td>
		  	</tr>
		<!--end:log_row-->
		<!--end:log_body-->
		</tbody>
	</table>
	<!--end:log-error-->
	<!--start:log-login-->
